{
  "globalShortcut": "Cmd+\\",
  "mcpServers": {
    "mcp-installer": {
      "command": "sh",
      "args": ["-c", "eval \"$(fnm env)\" && npx -y @anaisbetts/mcp-installer"]
    },
    "mcp-youtube": {
      "command": "sh",
      "args": ["-c", "eval \"$(fnm env)\" && npx -y @anaisbetts/mcp-youtube"]
    },
    "desktop-commander": {
      "command": "sh",
      "args": [
        "-c",
        "eval \"$(fnm env)\" && npx -y @smithery/cli@latest run @wonderwhy-er/desktop-commander --key ${smithery_key}"
      ]
    },
    "server-sequential-thinking": {
      "command": "sh",
      "args": [
        "-c",
        "eval \"$(fnm env)\" && npx -y @smithery/cli@latest run @smithery-ai/server-sequential-thinking --key ${smithery_key}"
      ]
    },
    "think-mcp-server": {
      "command": "sh",
      "args": [
        "-c",
        "eval \"$(fnm env)\" && npx -y @smithery/cli@latest run @PhillipRt/think-mcp-server --key ${smithery_key}"
      ]
    }
  }
}
